{
  "name": "cdk_template",
  "version": "0.1.0",
  "main": "dist/lib/app.js",
  "types": "dist/types/app.d.ts",
  "bin": {
    "cdk_template": "bin/cdk_template.js"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf cdk.out",
    "clean:workspace": "rm -rf dist && rm -rf cdk.out && rm -rf node_modules && rm package-lock.json",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint:fix": "npx eslint '**/*.ts' --fix",
    "prettier": "prettier --config .prettierrc './**/*.ts' --write",
    "check": "npx npm-check-updates \"/aws-cdk/\"",
    "upgrade": "npx npm-check-updates -u \"/aws-cdk/\" --reject='aws-cdk-lib,constructs' && npm install",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/node": "17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "prettier-plugin-multiline-arrays": "^1.1.1",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "typescript": "~4.7.3"
  },
  "dependencies": {
    "@meta-search/kaito-cdk-construct": "^0.0.112",
    "aws-cdk-lib": "^2.27.0",
    "constructs": "^10.1.31",
    "source-map-support": "^0.5.21"
  }
}
